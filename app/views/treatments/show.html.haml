= content_for :header_title do
  = @treatment.id

%table.table.table-striped
  %thead
    %tr
      %th ID
      %th Question
      %th Answer
      %th Alchemy - Keywords
      %th Alchemy - Keywords
  %tbody
    - @treatment.questions.each do |question|
      - question.answers.each do |answer|
        = render 'question_answer', answer: answer, question: question
