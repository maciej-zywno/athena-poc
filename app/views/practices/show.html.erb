<h2> <%= @practice.name %> </h2>
<h1> Departments: </h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Credit Card types</th>
      <th>Medication history consent</th>
      <th>Time zone offset</th>
      <th>Provider group id</th>
      <th>Single appointment contract max</th>
      <th>State</th>
      <th>Portal url</th>
      <th>City</th>
      <th>Place of service facility</th>
      <th>One year contract max</th>
      <th>Latitude</th>
      <th>Provider group name</th>
      <th>Does not observe dst</th>
      <th>Address</th>
      <th>Place of service type id</th>
      <th>Longitude</th>
      <th>Clinicals</th>
      <th>Timezone</th>
      <th>Patient department name</th>
      <th>Name</th>
      <th>Place of service type name</th>
      <th>Phone</th>
      <th>Ecommerce credit card types</th>
      <th>Zip</th>
      <th>Communicator brand id</th>
    </tr>
  </thead>
  <tbody>
    <% @departments.each do |department| %>
      <tr>
        <td> <%= link_to department.departmentid, practice_department_path(params[:id], department.departmentid) %> </td>
        <td> <%= department.creditcardtypes %> </td>
        <td> <%= department.medicationhistoryconsent %> </td>
        <td> <%= department.timezoneoffset %> </td>
        <td> <%= department.providergroupid %> </td>
        <td> <%= department.singleappointmentcontractmax %> </td>
        <td> <%= department.state %> </td>
        <td> <%= department.portalurl %> </td>
        <td> <%= department.city %> </td>
        <td> <%= department.placeofservicefacility %> </td>
        <td> <%= department.oneyearcontractmax %> </td>
        <td> <%= department.latitude %> </td>
        <td> <%= department.providergroupname %> </td>
        <td> <%= department.doesnotobservedst %> </td>
        <td> <%= department.address %> </td>
        <td> <%= department.placeofservicetypeid %> </td>
        <td> <%= department.longitude %> </td>
        <td> <%= department.clinicals %> </td>
        <td> <%= department.timezone %> </td>
        <td> <%= department.patientdepartmentname %> </td>
        <td> <%= department.name %> </td>
        <td> <%= department.placeofservicetypename %> </td>
        <td> <%= department.phone %> </td>
        <td> <%= department.ecommercecreditcardtypes %> </td>
        <td> <%= department.zip %> </td>
        <td> <%= department.communicatorbrandid %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

